{
  "name": "dev",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "develop",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "develop",
        "command": "./run-dev.sh"
      }
    },
    "down": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "develop",
        "command": "docker-compose down"
      }
    },
    "up": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "develop",
        "command": "docker-compose up --build -d"
      }
    }
  },
  "tags": ["infrastructure"]
}
